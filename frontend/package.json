    "babel-loader": "^8.0.1",
    "coveralls": "^3.0.2",
    "graphql-request": "^1.8.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.5",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "regenerator-runtime": "^0.12.1",
    "standard": "^12.0.1",
